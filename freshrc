# vim: set ft=sh:

fresh freshshell/fresh bin/fresh --bin

fresh-options --file=~/.zshrc --marker
  fresh shell/zsh/zshrc.zsh
  fresh shell/generic.sh
fresh-options

fresh-options --file=~/.vimrc --marker=\"
  fresh vim/vimrc
  fresh twe4ked/dotfiles vim/vundle_before.vim
  fresh vim/vundle.vim
  fresh twe4ked/dotfiles vim/vundle_after.vim
fresh-options

fresh-options --bin
  fresh vim-scripts/vimcat vimcat
fresh-options

fresh robbyrussell/oh-my-zsh . --file=~/.oh-my-zsh/
fresh shell/zsh/oh-my-zsh/custom --file=~/.oh-my-zsh_custom/
