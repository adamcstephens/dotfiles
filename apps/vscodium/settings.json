{
  "[elixir]": {
    "editor.defaultFormatter": "elixir-tools.elixir-tools"
  },
  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "[html-eex]": {
    "editor.defaultFormatter": "elixir-tools.elixir-tools"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordBasedSuggestionsMode": "currentDocument"
  },
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "[phoenix-heex]": {
    "editor.defaultFormatter": "elixir-tools.elixir-tools"
  },
  "[properties]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[yaml]": {
    "editor.wordBasedSuggestionsMode": "currentDocument"
  },
  "[yuck]": {
    "editor.defaultFormatter": "eww-yuck.yuck"
  },
  "breadcrumbs.enabled": true,
  "calva.autoConnectRepl": true,
  "calva.projectRootsSearchExclude": [
    "bower_components",
    ".bzr",
    ".cache",
    ".ccls-cache",
    ".clangd",
    ".classpath",
    "*.code-search",
    ".cpcache",
    "_darcs",
    ".DS_Store",
    ".ensime_cache",
    ".eunit",
    "flow-typed",
    "_FOSSIL_",
    ".fslckout",
    ".git",
    ".hg",
    ".idea",
    ".joyride",
    "node_modules",
    ".pijul",
    ".project",
    ".shadow-cljs",
    ".stack-work",
    ".svn",
    "target",
    ".tox",
    ".vscode",
    ".direnv"
  ],
  "calva.showCalvaSaysOnStart": false,
  "debug.internalConsoleOptions": "neverOpen",
  "diffEditor.renderSideBySide": true,
  "direnv.restart.automatic": true,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "JetBrains Mono, Noto Color Emoji, Symbols Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontFamily": "default",
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    120
  ],
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "editorconfig.generateAuto": false,
  "elixirLS.fetchDeps": true,
  "emmet.includeLanguages": {
    "html-eex": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/*.bst": "yaml",
    "*.env": "shellscript",
    "*.keymap": "c",
    "*.overlay": "c",
    "*.sls": "yaml",
    "*.yaml.gotmpl": "yaml",
    "Gemfile*": "ruby",
    "Jenkinsfile*": "groovy",
    "Pipefile": "toml"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "github.gitAuthentication": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.views.repositories.autoRefresh": false,
  "go.lintFlags": [
    "--fast"
  ],
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "haskell.manageHLS": "PATH",
  "javascript.format.semicolons": "remove",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "nix.enableLanguageServer": true,
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": {
      "formatting": {
        "command": [
          "nixfmt"
        ]
      }
    },
    "nixd": {
      "formatting": {
        "command": "nixfmt"
      }
    }
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.venvPath": "~/.local/share/virtualenvs",
  "redhat.telemetry.enabled": false,
  "remote.SSH.defaultExtensions": [
    "esbenp.prettier-vscode",
    "jnoortheen.nix-ide",
    "mkhl.direnv",
    "foxundermoon.shell-format",
    "redhat.vscode-yaml",
    "timonwong.shellcheck",
    // "lexical-lsp.lexical",
    "skellock.just",
    "golf1052.base16-generator"
  ],
  "remote.SSH.lockfilesInTmp": true,
  "remote.SSH.remotePlatform": {
    "blank": "linux",
    "crank": "linux",
    "funk": "linux",
    "lxd1": "linux"
  },
  "scm.defaultViewMode": "tree",
  "search.defaultViewMode": "tree",
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "shellcheck.disableVersionCheck": true,
  "shellcheck.exclude": [
    "SC1117",
    "SC2039"
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "kitty",
  "terminal.external.osxExec": "Kitty.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 20000,
  "terminal.integrated.tabs.enabled": true,
  "terraform.languageServer.enable": true,
  "terraform.languageServer.path": "",
  "update.mode": "none",
  "window.autoDetectColorScheme": true,
  "window.nativeFullScreen": false,
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "[Base16 Ayu Dark]": {
      "button.foreground": "#272D38",
      "button.secondaryForeground": "#272D38",
      "editorCodeLens.foreground": "#BFBDB6",
      "statusBar.background": "#272D38",
      "statusBar.noFolderBackground": "#0F1419",
      "statusBarItem.remoteBackground": "#3E4B59",
      "tab.activeBackground": "#272D38",
      "tab.activeBorder": "#BFBDB6"
    },
    "[Sonokai Andromeda]": {
      "gitDecoration.ignoredResourceForeground": "#565864",
      "quickInputList.focusBackground": "#363a4e80",
      "terminal.ansiBlack": "#181a1c",
      "terminal.ansiBrightBlack": "#7e8294",
      "terminal.background": "#2b2d3a"
    }
  },
  "workbench.editor.decorations.badges": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.showIcons": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.preferredDarkColorTheme": "GitHub Dark Colorblind (Beta)",
  "workbench.preferredLightColorTheme": "GitHub Light Colorblind (Beta)",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "always",
  "yaml.customTags": [
    "!encrypted/pkcs1-oaep scalar",
    "!vault scalar"
  ],
  "yaml.schemas": {
    "https://json.schemastore.org/taskfile.json": "taskfiles/*"
  },
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "workbench.editor.empty.hint": "hidden",
  "editor.wordWrap": "on",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorTheme": "GitHub Dark Colorblind (Beta)"
}
